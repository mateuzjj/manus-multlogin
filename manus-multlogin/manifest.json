{
  "manifest_version": 3,
  "name": "Cookie Switcher",
  "version": "1.2",
  "description": "Troca cookies para dom√≠nio manus.im",
  "permissions": [
    "cookies",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "*://*.manus.im/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
